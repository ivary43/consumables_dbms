<% include ../partials/header %> 
<div class="container" style="margin-top: 25px">
 <div class="row">
    <table class ="table table-striped table-bordered table-hover">
        <h1 class="text-center">Inventory Update History</h1>
            <thead>
                <tr>
                    <th>Sr.no</th>
                    <th>Item</th>
                    <th>Updated from</th>
                     <th>Updated by</th>
                     <th> Date</th>
                </tr>
            </thead>

            <tbody>
                <%var i =0; histories.forEach((history)=>{  %>
                
                 <tr>
                    <td> <%= i+1 %> </td>
                    <td> <%= history.name %> </td>
                    <td> <%= history.curr_quantity %></td>
                    <td> <%= history.updatedBy %></td>
                    <td> <%= history.dateUpdated.toLocaleString() %></td>
            
                 </tr>   
               <% i++;}) %>
            </tbody>

    </table>
</div>
<% include ../partials/footer %> 